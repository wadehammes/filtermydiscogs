{
  "name": "filtermydiscogs",
  "version": "0.0.1",
  "packageManager": "pnpm@10.28.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 6767 --webpack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start -p 6767",
    "tsc:ci": "tsc --strict",
    "lint": "biome check src",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --write src",
    "format": "biome format src",
    "format:fix": "biome format --write src",
    "test": "jest --runInBand --detectOpenHandles",
    "test:ci": "jest --passWithNoTests",
    "test:file": "jest --runInBand --detectOpenHandles --watch --",
    "scaffold": "scripts/scaffold.sh",
    "db:generate": "prisma generate",
    "db:push": "bash scripts/load-env-and-push.sh",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:prod": "dotenv -e .env.local -- prisma migrate dev --name init",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:pull:dev": "bash scripts/pull-db-env.sh development",
    "db:pull:prod": "bash scripts/pull-db-env.sh production",
    "db:validate": "dotenv -e .env.local -- prisma validate",
    "analyze": "ANALYZE=true pnpm build",
    "bundle-report": "pnpm build && npx @next/bundle-analyzer",
    "lighthouse": "npx lighthouse http://localhost:6767 --output=html --output-path=./lighthouse-report.html",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "9.9.0",
    "@next/third-parties": "^16.1.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.3.2",
    "html-react-parser": "^5.2.11",
    "isbot": "^5.1.32",
    "js-cookie": "^3.0.5",
    "lodash.flatten": "^4.4.0",
    "next": "16.1.1",
    "oauth-1.0a": "^2.2.6",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-intersection-observer": "^10.0.0",
    "recharts": "^3.6.0",
    "sharp": "^0.34.5",
    "usehooks-ts": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@jest/test-sequencer": "30.2.0",
    "@jest/types": "^30.2.0",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "^16.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/jest-when": "^3.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash.flatten": "^4.4.9",
    "@types/node": "^25.0.6",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "critters": "^0.0.25",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-location-mock": "^2.0.0",
    "jest-mock": "^30.2.0",
    "jest-transformer-svg": "^2.1.0",
    "jest-when": "^3.5.2",
    "lighthouse": "^13.0.1",
    "postcss": "^8.4.20",
    "postcss-custom-media": "^11.0.6",
    "prisma": "^7.2.0",
    "react-is": "^19.2.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
