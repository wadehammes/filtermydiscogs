{
  "name": "filtermydiscogs",
  "version": "0.0.1",
  "packageManager": "pnpm@10.18.3",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "export": "next export",
    "tsc:ci": "tsc --strict",
    "lint": "biome check src",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --write src",
    "format": "biome format src",
    "format:fix": "biome format --write src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "test:file": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles --watch --",
    "scaffold": "scripts/scaffold.sh",
    "analyze": "ANALYZE=true pnpm build",
    "bundle-report": "pnpm build && npx @next/bundle-analyzer",
    "lighthouse": "npx lighthouse http://localhost:7777 --output=html --output-path=./lighthouse-report.html",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/third-parties": "^15.5.6",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.3.2",
    "html-react-parser": "^5.2.7",
    "isbot": "^5.1.31",
    "js-cookie": "^3.0.5",
    "lodash.flatten": "^4.4.0",
    "next": "15.5.6",
    "oauth-1.0a": "^2.2.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-intersection-observer": "^9.4.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@jest/types": "^30.2.0",
    "@next/bundle-analyzer": "^15.5.6",
    "@next/eslint-plugin-next": "^15.5.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/jest-when": "^3.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash.flatten": "^4.4.9",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "critters": "^0.0.25",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-location-mock": "^2.0.0",
    "jest-mock": "^30.2.0",
    "jest-when": "^3.5.2",
    "lighthouse": "^13.0.0",
    "postcss": "^8.4.20",
    "react-is": "^19.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
